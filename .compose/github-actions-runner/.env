# GitHub repository URL for your project
GITHUB_URL=https://github.com/DeanLuus22021994/your-repo-name

# Runner token (get this from: https://github.com/DeanLuus22021994/your-repo-name/settings/actions/runners/new)
# Note: Tokens expire after 1 hour, so generate a new one when setting up
RUNNER_TOKEN=YOUR_RUNNER_TOKEN_HERE

# Runner name (optional, defaults to your-repo-runner)
RUNNER_NAME=your-repo-runner

# Work directory (optional, defaults to /actions-runner/_work)
RUNNER_WORKDIR=/actions-runner/_work

# Labels for the runner (optional, defaults to self-hosted,linux,x64)
LABELS=self-hosted,linux,x64,your-project

# Runner group (optional, defaults to Default)
RUNNER_GROUP=Default
