{
  "dashboard": {
    "title": "Redis Cache Metrics",
    "uid": "redis-metrics",
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Hit Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Connected Clients",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Clients",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Clients"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "legendFormat": "Used Memory",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes / 1024 / 1024",
            "legendFormat": "Max Memory",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Commands/sec",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[1m])",
            "legendFormat": "Commands",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Commands/s"},
          {"format": "short"}
        ]
      }
    ]
  },
  "overwrite": true
}
