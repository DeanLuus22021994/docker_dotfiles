{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  "line_length": false,
  "MD001": true,
  "MD002": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD005": true,
  "MD006": true,
  "MD007": {
    "indent": 2
  },
  "MD009": {
    "br_spaces": 2,
    "list_item_empty_lines": false,
    "strict": false
  },
  "MD010": {
    "code_blocks": true,
    "spaces_per_tab": 4
  },
  "MD011": true,
  "MD012": {
    "maximum": 2
  },
  "MD013": false,
  "MD014": true,
  "MD018": true,
  "MD019": true,
  "MD020": true,
  "MD021": true,
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD023": true,
  "MD024": {
    "siblings_only": true
  },
  "MD025": {
    "front_matter_title": ""
  },
  "MD026": {
    "punctuation": ".,;:!"
  },
  "MD027": true,
  "MD028": true,
  "MD029": {
    "style": "ordered"
  },
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD031": {
    "list_items": true
  },
  "MD032": true,
  "MD033": {
    "allowed_elements": [
      "a",
      "abbr",
      "br",
      "code",
      "div",
      "em",
      "img",
      "kbd",
      "span",
      "strong",
      "sub",
      "sup",
      "details",
      "summary"
    ]
  },
  "MD034": true,
  "MD035": {
    "style": "---"
  },
  "MD036": true,
  "MD037": true,
  "MD038": true,
  "MD039": true,
  "MD040": true,
  "MD041": {
    "front_matter_title": ""
  },
  "MD042": true,
  "MD043": false,
  "MD044": {
    "names": [
      "Docker",
      "Python",
      "JavaScript",
      "TypeScript",
      "PostgreSQL",
      "MariaDB",
      "Redis",
      "Nginx",
      "MkDocs",
      "GitHub",
      "VS Code"
    ],
    "code_blocks": false,
    "html_elements": false
  },
  "MD045": true,
  "MD046": {
    "style": "fenced"
  },
  "MD047": true,
  "MD048": {
    "style": "backtick"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  },
  "MD051": true,
  "MD052": true,
  "MD053": true,
  "no-hard-tabs": true,
  "whitespace": true
}
